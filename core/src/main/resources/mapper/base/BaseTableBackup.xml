<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.figure.core.repository.base.BaseTableBackupMapper">

        <!-- 通用查询映射结果 -->
        <resultMap id="BaseResultMap" type="com.figure.core.model.base.BaseTableBackup">
                <result column="id" property="id" />
                    <result column="type" property="type" />
                    <result column="tablename" property="tablename" />
                    <result column="dataName" property="dataName" />
                    <result column="conditions" property="conditions" />
        </resultMap>

        <!-- 通用查询结果列 -->
        <sql id="Base_Column_List">
                id,
            type, tablename, dataName, conditions
        </sql>

</mapper>
